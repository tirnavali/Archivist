<p style="color: green"><%= notice %></p>

<%= render @document %>
<br/>
<div>
  <%= link_to "Edit this document", edit_document_path(@document), class: "ui yellow button" %> |
  <%= link_to "Back to documents", documents_path, class: "ui button" %>

  <%= button_to "Destroy this document", @document, method: :delete, class: "ui red button" %>
</div>
